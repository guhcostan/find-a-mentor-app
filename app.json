        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./assets/google-services.json",
      "package": "com.vixz.findamentor"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
